{
  "env": {},
  "apps": [
    {
      "name": "Desktop",
      "image-path": "desktop",
      "exclude-global-prep-cmd": "false",
      "elevated": "",
      "auto-detach": "true",
      "wait-all": "true",
      "exit-timeout": "5",
      "menu-cmd": [
        {
          "id": "kcENAT5r9P",
          "name": "Open Touch Keyboard",
          "cmd": ".\\tools\\qiin-tabtip.exe",
          "elevated": "false"
        },
        {
          "id": "rjeOKHmcdL",
          "name": "DPI 250",
          "cmd": ".\\tools\\SetDpi.exe 250",
          "elevated": "false"
        },
        {
          "id": "C_vunhdQwA",
          "name": "DPI 100",
          "cmd": ".\\tools\\SetDpi.exe 100",
          "elevated": "false"
        }
      ]
    },
    {
      "name": "Steam Big Picture",
      "cmd": "steam://open/bigpicture",
      "auto-detach": "true",
      "wait-all": "true",
      "image-path": "steam.png"
    },
    {
      "name": "Disable Virtual Display",
      "cmd": ".\\tools\\device-toggler.exe 2 2 \"Zako Display Adapter\"",
      "elevated": "true"
    },
    {
      "name": "Restart Sunshine in Safe Mode",
      "cmd": ".\\Sunshine.exe -1"
    }
  ]
}
